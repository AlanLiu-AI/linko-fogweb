{
    "parser":  "@typescript-eslint/parser", 
    "plugins": [
      "typescript"

    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [          
        "plugin:@typescript-eslint/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "tsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
  "rules": {

  }
}